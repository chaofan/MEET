<%= simple_form_for([:admin, @admin_user]) do |f| %>
  <%= f.error_messages %>

  <div class="inputs">
    <%= f.input :email %>
    <%= f.association :roles, :input_html => { :size=>6 } %>
    <%= f.input :created_at %>
  </div>

  <div class="actions">
    <%= f.button :submit %>
  </div>
<% end %>
