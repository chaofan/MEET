<%= simple_form_for([:admin, @admin_user]) do |f| %>
  <%= f.error_messages %>

  <%= f.input :email %>
  <%= f.association :roles, :as=>:check_boxes, :input_html => { :size=>6 } %>
  <%= f.input :created_at %>

  <div class="actions">
    <%= f.button :submit, :class=>"ml140" %>
  </div>
<% end %>
