<%= simple_form_for @profile, :html => { :multipart => true } do |f| %>
  <%= f.error_messages %> 

  <%= f.input :name %>
  <%= f.input :english_name %>
  <%= f.input :birthday %>
  <%= f.input :star, :collection=>['双子座','天平座'] %>
  <%= f.input :sex, :collection=>['男', '女'] %>
  <%= f.input :avatar,  :as=>:file %>

  <%= f.button :submit %>
<% end %>


