<%= simple_form_for @profile do |f| %>
  <%= f.error_messages %> 

  <%= f.input :address %>
  <%= f.input :mobile %>
  <%= f.input :home_phone %>
  <%= f.input :home_province,  :collection=> ["上海", "北京"] %>
  <%= f.input :home_city,  :collection=> ["浦东新区", "徐汇区"]  %>

  <%= f.input :msn %>
  <%= f.input :qq %>
  <%= f.input :gmail %>
  <%= f.input :blog %>
  <%= f.input :homepage %>


  <%= f.button :submit %>
<% end %>

