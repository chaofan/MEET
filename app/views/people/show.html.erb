<% content_for :header do %>
  <%= stylesheet_link_tag	"userInfo" %>
<% end %>

<% if @user == current_user %>
  <%= render_cell :people, :current_people, :current_user=>current_user, :user=>@user %>
<% else %>
  <%= render_cell :people,  :other_people, :current_user=>current_user, :user=>@user %>
<% end %>
